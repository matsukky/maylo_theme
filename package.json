{
	"name": "maylotheme",
	"version": "1.0.0",
	"publisher": "matsukky",
	"description": "A chill and work wanted theme, base of Ros√© Pine, but in a different way.",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/matsukky/maylo_theme"
	},
	"author": {
		"name": "Maylody",
		"email": "code@matsukky.com"
	},
	"type": "module",
	"engines": {
		"vscode": "^1.57.0"
	},
	"scripts": {
		"release": "npx np@latest --no-publish --no-tests",
		"version": "npm run build && npm run format"
	},
	"icon": "assets/icon.png",
	"displayName": "Maylo Theme",
	"categories": [
		"Themes"
	],
	"keywords": [
		"minimal",
		"aesthetic",
		"pastel",
		"classy",
		"soft"
	],
	"galleryBanner": {
		"color": "#191724",
		"theme": "dark"
	},
	"contributes": {
		"themes": [
			{
				"label": "Maylo Theme",
				"uiTheme": "vs-dark",
				"path": "./themes/maylotheme-color-theme.json"
			},
			{
				"label": "Maylo Theme (no italics)",
				"uiTheme": "vs-dark",
				"path": "./themes/maylotheme-noitalics-color-theme.json"
			}
		]
	},
	"devDependencies": {
		"prettier": "^3.3.3"
	},
	"prettier": {
		"semi": false,
		"singleQuote": true,
		"useTabs": true
	}
}
